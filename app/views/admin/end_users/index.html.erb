<div class="container">
  <div class="row">
    <div class="col-10">

      <h4>会員一覧</h4>
      <table class="table">
        <thead>
          <tr>
            <th>会員ID</th>
            <th>名前</th>
            <th>メールアドレス</th>
            <th>ステータス</th>
          </tr>
        </thead>
        <tbody>
          <% @end_users.each do |end_user| %>
            <tr>
              <td><%= end_user.id %></td>
              <td><%= end_user.last_name %><%= end_user.first_name %></td>
              <td><%= end_user.email %></td>
              <% if end_user.is_deleted == false %>
                <td class="text-success">有効</td>
              <% else %>
                <td class="text-danger">退会済み</td>
              <% end %>
            </tr>
          <% end %>
        </tbody>
      </table>

    </div>
  </div>
</div>